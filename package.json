{
  "name": "rt-puc",
  "description": "React Transition Portal-based UI Components",
  "version": "1.0.5",
  "type": "module",
  "license": "MIT",
  "packageManager": "yarn@4.0.2",
  "main": "./dist/rt-puc.umd.js",
  "module": "./dist/rt-puc.es.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/J-Dany/rt-puc.git"
  },
  "exports": {
    ".": {
      "import": "./dist/rt-puc.es.js"
    },
    "./utils": {
      "import": "./dist/rt-puc-utils.es.js"
    }
  },
  "keywords": [
    "react",
    "transition",
    "addons",
    "transition-group",
    "animation",
    "css",
    "transitions"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
